{

  "description": "Firefox extension that adds the steam links of a game to the backloggd website.",
  "manifest_version": 3,
  "name": "Steam integration for Backloggd",
  "version": "1.0",
  "homepage_url": "https://github.com/COwOkies/Steam-integration-for-Backloggd",
  "icons": {
    "48": "icons/logo-48.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.backloggd.com/games/*"],
      "js": ["script.js"]
    }
  ]

}
